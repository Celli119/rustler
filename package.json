{
  "name": "rustler",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:x11": "GDK_BACKEND=x11 WAYLAND_DISPLAY= bunx tauri dev",
    "tauri:dev": "bunx tauri dev",
    "tauri:dev:x11": "GDK_BACKEND=x11 WAYLAND_DISPLAY= bunx tauri dev",
    "build": "tsgo -b && vite build",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "preview": "vite preview",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "prepare": "husky",
    "tauri": "tauri",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/nunito-sans": "^5.2.7",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-positioner": "^2.3.1",
    "@tauri-apps/plugin-process": "^2.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.0",
    "@commitlint/config-conventional": "^19.7.0",
    "@eslint/js": "^9.39.1",
    "@tauri-apps/cli": "^2.9.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260124.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "oxlint --fix",
      "oxfmt --write"
    ],
    "*.{json,md,yml,yaml}": [
      "oxfmt --write"
    ],
    "*.rs": [
      "cargo fmt --manifest-path src-tauri/Cargo.toml --"
    ]
  }
}
